<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
    <context id="fddNhCWebCenter" targetRuntime="MyBatis3">
        <plugin type="com.itfsw.mybatis.generator.plugins.LimitPlugin"/>
        <plugin type="com.itfsw.mybatis.generator.plugins.BatchInsertPlugin"/>
        <plugin type="com.itfsw.mybatis.generator.plugins.ModelColumnPlugin"/>
        <commentGenerator>
            <!--<property name="suppressAllComments" value="false"/>-->
            <property name="addRemarkComments" value="true"/>
        </commentGenerator>

        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://10.0.4.105/xdchen_test?useUnicode=true&amp;characterEncoding=utf8"
                        userId="xuelili" password="xuelilifdd"/>

        <javaTypeResolver>
            <property name="forceBigDecimals" value="false"/>
        </javaTypeResolver>
        <javaModelGenerator targetPackage="com.zidongxiangxi.pratice.two.entity" targetProject="src/main/java">
            <property name="enableSubPackages" value="true"/>
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>

        <sqlMapGenerator targetPackage="mybatis" targetProject="src/main/resources">
            <property name="enableSubPackages" value="true"/>
        </sqlMapGenerator>

        <javaClientGenerator type="XMLMAPPER" targetPackage="com.zidongxiangxi.pratice.two.dao"
                             targetProject="src/main/java">
            <property name="enableSubPackages" value="true"/>
        </javaClientGenerator>


        <table tableName="text" domainObjectName="Text"
               enableSelectByExample="false" enableDeleteByExample="false" enableCountByExample="false"
               enableUpdateByExample="false" enableDeleteByPrimaryKey="false">
            <generatedKey column="id" sqlStatement="MySql" identity="true" />
        </table>
 </context>
</generatorConfiguration>
